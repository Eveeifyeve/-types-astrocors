{
	"compilerOptions": {
		"lib": ["esnext", "dom"],
		"module": "ESNext",
		"moduleResolution": "Node",
		"strict": true,
		"types": ["./index.d.ts", "@types/node"],
		"target": "esnext",
		"allowSyntheticDefaultImports": true,
		"noUnusedLocals": true,
		"noEmit": true,
		"resolveJsonModule": true
	},
	"files": ["index.d.ts"],
	"extends": "astro/tsconfigs/base"
}
